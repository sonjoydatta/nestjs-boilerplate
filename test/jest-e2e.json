{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/jwt/(.*)": "<rootDir>/../libs/jwt/src/$1",
    "@libs/jwt": "<rootDir>/../libs/jwt/src",
    "@libs/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
    "@libs/utils": "<rootDir>/../libs/utils/src",
    "@libs/redis/(.*)": "<rootDir>/../libs/redis/src/$1",
    "@libs/redis": "<rootDir>/../libs/redis/src"
  }
}